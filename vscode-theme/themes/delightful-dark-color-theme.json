  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#76706c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff78be"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#9e9e9e"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.template",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#ffcc5a"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#7ad689"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#ff7076"
      }
    },
    {
      "name": "Escape characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#ff7076"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#00c3db"
      }
    },
    {
      "name": "Types and classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#22cde5"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#ede9e4"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#e89dc0"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#ede9e4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags (HTML/JSX)",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#ff78be"
      }
    },
    {
      "name": "Attributes (HTML)",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#ffcc5a"
      }
    },
    {
      "name": "CSS property names",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss"
      ],
      "settings": {
        "foreground": "#00c3db"
      }
    },
    {
      "name": "CSS values",
      "scope": [
        "support.constant.property-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#7ad689"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#f44f5d"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#a09a94"
      }
    },
    {
      "name": "Template expression",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#ff78be"
      }
    },
    {
      "name": "Markdown heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#ede9e4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#ede9e4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#a09a94",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#ff78be"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#ff7076"
      }
    },
    {
      "name": "Markdown list",
      "scope": [
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#ede9e4"
      }
    },
    {
      "name": "JSON property name",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#00c3db"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#ff7076",
    "parameter": {
      "foreground": "#ede9e4",
      "italic": true
    },
    "property.declaration": "#e89dc0",
    "function.declaration": {
      "foreground": "#00c3db",
      "bold": true
    },
    "type": "#22cde5",
    "interface": "#22cde5",
    "enum": "#22cde5",
    "enumMember": "#ff7076",
    "namespace": "#a09a94"
  }
}
