  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#7a736e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#cd0087"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#534b45"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.template",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#936e00"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#007f30"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#db173d"
      }
    },
    {
      "name": "Escape characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#db173d"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#008394"
      }
    },
    {
      "name": "Types and classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#00a1b5"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#1b150f"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#a11d6c"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#1b150f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags (HTML/JSX)",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#cd0087"
      }
    },
    {
      "name": "Attributes (HTML)",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#936e00"
      }
    },
    {
      "name": "CSS property names",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss"
      ],
      "settings": {
        "foreground": "#008394"
      }
    },
    {
      "name": "CSS values",
      "scope": [
        "support.constant.property-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#007f30"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ed324b"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#534b45"
      }
    },
    {
      "name": "Template expression",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#cd0087"
      }
    },
    {
      "name": "Markdown heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#1b150f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#1b150f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#534b45",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#cd0087"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#db173d"
      }
    },
    {
      "name": "Markdown list",
      "scope": [
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#1b150f"
      }
    },
    {
      "name": "JSON property name",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#008394"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#db173d",
    "parameter": {
      "foreground": "#1b150f",
      "italic": true
    },
    "property.declaration": "#a11d6c",
    "function.declaration": {
      "foreground": "#008394",
      "bold": true
    },
    "type": "#00a1b5",
    "interface": "#00a1b5",
    "enum": "#00a1b5",
    "enumMember": "#db173d",
    "namespace": "#534b45"
  }
}
